config_version=5

[application]

config/name="DungeonOfGuns"
config/version="0.0.0"
run/main_scene="res://Intro.tscn"
config/features=PackedStringArray("4.2", "C#", "Forward Plus")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
config/icon="res://icon.svg"

[aseprite]

import/import_plugin/default_automatic_importer="SpriteFrames"

[autoload]

NetworkManager="*res://Scripts/Network/NetworkManager.cs"
SteamManager="*res://Scripts/Network/SteamManager.cs"
SteamLobbyManager="*res://Scripts/Network/SteamLobbyManager.cs"
MainThreadInvoker="*res://Scripts/MainThreadInvoker.cs"
DiscordHandler="*res://Scripts/DiscordHandler.cs"

[display]

window/size/viewport_width=550
window/size/viewport_height=320
window/size/window_width_override=825
window/size/window_height_override=480
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="DungeonOfGuns"

[editor_plugins]

enabled=PackedStringArray("res://addons/AsepriteWizard/plugin.cfg")

